{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.wasm",
        "headers": [{
          "key": "Content-Type",
          "value": "application/wasm"
        }]
      }
    ]
  }
}
